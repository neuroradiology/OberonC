module Programa9;
var a, b : boolean;

procedure printBool( bool: boolean );
begin
	if bool then
		writeln("true");
	else
		writeln("false");
	end;
end printBool;

begin
	a := true;
	b := ( a and ( not a ));
	
	printBool ( a );
	printBool ( b );
	printBool ( a or b );
	
end Programa9.
