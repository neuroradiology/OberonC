module Programa9;
var a, b : boolean;

procedure printBool( bool: boolean );
begin
	if bool then
		writeln("true");
	else
		writeln("false");
	end;
end printBool;

function negate( bool: boolean ): boolean;
begin
	return not bool;
end negate;

begin
	a := true;
	b := negate( a );
	
	printBool ( a );
	printBool ( b );
	printBool ( a or b );
	printBool ( a and b );
	printBool ( not b );
	
end Programa9.
